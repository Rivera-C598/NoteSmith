# NoteSmith Configuration File
# Copy this file to 'config.properties' and update with your settings

# Database Configuration
db.url=jdbc:mysql://localhost:3306/notesmith_db?useSSL=false&serverTimezone=UTC
db.user=root
db.password=
db.pool.size=10

# Security Settings
security.max.login.attempts=5
security.password.min.length=8

# AI Configuration (Gemini API)
ai.enabled=true
ai.gemini.api.key=YOUR_GEMINI_API_KEY_HERE
ai.gemini.model=gemini-2.5-flash-latest
ai.cache.enabled=true
ai.max.tokens=1000
